<h1>Listing musics</h1>

<%= link_to "Log Out", destroy_user_session_path, method: :delete %>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Musician</th>
      <th>Imageurl</th>
      <th>Videourl</th>
      <th>Color</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @musics.each do |music| %>
      <tr>
        <td><%= music.title %></td>
        <td><%= music.description %></td>
        <td><%= music.musician %></td>
        <td><%= music.imageUrl %></td>
        <td><%= music.videoUrl %></td>
        <td><%= music.color %></td>
        <td><%= link_to 'Show', music %></td>
        <td><%= link_to 'Edit', edit_music_path(music) %></td>
        <td><%= link_to 'Destroy', music, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Music', new_music_path %>
